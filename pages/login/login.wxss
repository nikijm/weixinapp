/* pages/login/login.wxss */
.checkBox{
    width: 100%;
    height: 90rpx;
    color: red;
    font-weight: 700;
    font-size: 10pt;
    display: flex;
    align-items: center;
    justify-content: center;
}
.content{
    width: 100%;
    background-color: #f5f5f5;
}
.top{
    width: 100%;
    height: 303rpx;

}
.top_top{
    width: 100%;
    height: 243rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}
.top_down{
    width: 90%;
    margin:auto;
    height: 60rpx;
    display: flex;
    font-size: 11pt;
    justify-content: space-between;
    color: #cdcdcd;
    font-weight: 700;

}
.input_box{
    width: 100%;
    height: 209rpx;
    background-color: white;
    font-size: 11pt;

}
.acc_box{
    width:90%;
    margin:auto;
    display: flex;
    flex-direction: row;
    align-items: center;
    height: 50%;
    border-bottom:1px solid #cdcdcd;
}
.acc_box>label{
    width: 25%;
    height: 100%;
     display: flex;
    align-items: center;
}
.acc_box>input{
    width: 100%;
    height: 100%;
}
.pwd_box{
    border-bottom:0;
}
button{
    width: 90%;
background-color: #f14d41;
margin-top: 43rpx;
color: white;
}
.reg{
    width: 90%;
    height: 100rpx;
    margin: auto;
    display: flex;
    align-items: center;
    justify-content: space-between;
    color: #d36960;
    font-weight: 700;
    font-size: 11pt;
    
}
.footer{
    width: 100%;
    height: 337rpx;
}
.footer_title{
    width: 85%;
    height: 115rpx;
    margin: auto;
    display: flex;
    align-items: center;
    justify-content: space-around;
    font-size: 11pt;
    color:#a9a9a9;

}
.logos{
width: 530rpx;
height: 153rpx;
margin: auto;
display: flex;
flex-direction: row;
justify-content: space-between;
}
.weixin{
    width: 94rpx;
    height: 153rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items: center;
    font-size: 10pt;
}
.weixin>image{
    width: 92rpx;
    height: 87rpx;

}